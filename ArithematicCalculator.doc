public class ArithematicCalculator {
	public int a,b,c;
	public void Addition()
	{
		c=a+b;
		System.out.println("Addition of two numbers is: "+c);
		
	}
	public void Subtraction()
	{
		c=a-b;
		System.out.println("Difference between two numbers is: "+c);
	
	}
	public void Multiplication()
	{
		c=a*b;
		System.out.println("Product of two numbers is: "+c);
	}
	public void Division()
	{
		c=a/b;
		System.out.println("Division of two numbers is: "+c);
	}
	
}
public class ArithematicCalculatorMain {
	public static void main(String[] args) {
		ArithematicCalculator ac=new ArithematicCalculator();
		Scanner s = new Scanner(System.in);
		int n;
		System.out.println("1.Addition\n2.Subtraction\n3.Multiplication\n4.Division");
		System.out.println("enter your choice");
		n=s.nextInt();
		System.out.println("enter two values");
		ac.a=s.nextInt();
		ac.b=s.nextInt();
		switch(n)
		{
		case 1:ac.Addition();
		break;
		case 2:ac.Subtraction();
		break;
		case 3:ac.Multiplication();
		break;
		case 4:ac.Division();
		break;
		default:System.out.println("you entered wrong choice");
		}
		
		
	}

}
